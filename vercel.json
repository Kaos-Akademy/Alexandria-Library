{
  "buildCommand": "npm install && cd frontend/library && npm install && npm run build",
  "outputDirectory": "frontend/library/dist",
  "installCommand": "npm install && cd frontend/library && npm install",
  "devCommand": "cd frontend/library && npm run dev",
  "functions": {
    "api/moonpay/sign-url.ts": {
      "runtime": "@vercel/node"
    },
    "api/moonpay/webhook.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/)(?!assets/).*)",
      "destination": "/index.html"
    }
  ]
}
